<AnchorPane prefHeight="513.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.lingen.modsim.KonkurrenzUmRessourcen.Charts.ScatterChartController">
